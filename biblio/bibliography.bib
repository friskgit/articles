	Keywords = {MESSIANISM, PHILOSOPHY, BIBLICAL teaching on idols & images, AESTHETICS, REPRESENTATION (Philosophy), SUBLIME, The, SIGNS & symbols, aesthetic theory, Dr Faustus, Jean-François Lyotard, Jewish thought, Messianism, representation, the image prohibition, the sublime, Theodor Adorno, Thomas Mann, ADORNO, Theodor W., 1903-1969, LYOTARD, Jean-Francois, 1924-1998, MANN, Thomas, 1875-1955, TEN commandments -- Images, DR. Faustus (Book)},
	Number = {5},
	Pages = {517 - 531},
	Title = {'If Adorno isn't the Devil, it's because he's a Jew': Lyotard's misreading of Adorno through Thomas Mann's Dr Faustus.},
	Volume = {35},
	URL = {http://ludwig.lub.lu.se/login?url=https://search-ebscohost-com.ludwig.lub.lu.se/login.aspx?direct=true&db=poh&AN=40102214&site=eds-live&scope=site},
