@article{clubb07,
	Abstract = {<SABS>The new Internet and mobile methods of
                  communications have changed the ways people
                  communicate and use language. In some cases,
                  information and communications technology, and
                  especially the Internet, have had a major effect on
                  use of the written languageabbreviations, new
                  methods of spelling, and methods of showing
                  emotions. Also, with the use of modern
                  communications technology, the way people interact
                  at a distance with language is shifting rapidly. The
                  premise of this work is that the Internet and mobile
                  technology have enabled several new forms of
                  communications that have radically changed how
                  people communicate with each other. These methods of
                  communication have allowed informal communication at
                  a distance rather than only face-to-face and in many
                  cases, virtually reduced distances.  In addition,
                  there are social implications that need to be
                  explored. This article introduces the concept of
                  human-to-computer-to-human interaction
                  (HCHI).</SABS>},
	Author = {Clubb, Orville Leverne},
