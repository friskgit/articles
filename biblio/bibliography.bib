@article{1994,
	Abstract = {The author describes "Music from the Center of the
                  Earth", a series of three electronic music
                  installations deploying underground loudspeakers in
                  various configurations. The works described are
                  "Notes from Underground", a tape piece realized in
                  conjunction with Melissa Gould's light installation,
                  "Floor Plan; The Magnetic Garden", a collective
                  musical earthwork united by the author's underground
                  sound chambers and musics emulating biological time;
                  and "The Listening Well", an interactive listening
                  and performing space that centers around a well
                  designed both to play back a continuous
                  sound-portrait of the world and to dialogue with
                  sounds "cast" in it by the public.},
