@article{Beverly1999,
	Abstract = {As concepts of reflexivity and postcolonial
                  perspectives have advanced our understandings of the
                  way we represent those we study, they have also
                  introduced a consciousness of the role of the self
                  in research. This article reviews the history of the
                  field of folklore with regard to the method of
                  obtaining data or texts and demonstrates that
                  collecting material contrasts with the practice of
                  conducting research in the field. Pointing to a
                  moment of transition, it shows that theories of
                  folklore had to undergo significant change before
                  methods of research would acknowledge the identity
                  of the fieldworker and its significance.},
	Author = {Beverly J. Stoeltje and Christie L. Fox and Stephen Olbrys},
	Journal = {The Journal of American Folklore},
	Number = 444,
	Pages = {158-182},
	Publisher = {University of Illinois Press},
	Title = {The Self in "Fieldwork": A Methodological Concern},
	Volume = 112,
	Year = 1999
}

